{	
    "name": "wiz_theme",
    "dataColors": ["#FFCF70", "#AC68D5", "#DCDCDC", "#ED5C7B", "#A7654A", "#B6B3B3"],
    "background": "#F9F4F1",
    "good":"#FFCF70",
    "neutral":"#AC68D5",
    "bad": "#ED5C7B",
    "visualStyles": {
        "*":{
            "*":{
                "background": [{"transparency": 100}],
                "categoryLabels": [{
                    "show" : false
                }],
                
                "title": [{
                    "show": true,
                    "fontColor": { "solid": { "color": "#808080" } },
                        "transparency": 100,
                    "alignment": "center",
                    "fontSize": 15,
                    "fontFamily": "Raleway Thin"
                    }],
                "valueAxis": [{
                        "showAxisTitle":false
                    }],
                "categoryAxis":[
                    {
                        "showAxisTitle":false
                    }
                ]
            }
        },
        "page": {
            "*": {
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#F9F4F1"
                            }
                        },
                        "transparency": 0
                    }
                ]
            }
        },
        "textbox": {
			"*": {
                "title": [{
                    "fontFamily":"Raleway Thin",
                    "divider": [{"show":false}]
                }],
				"*": [
					{
						"fontSize": 11,
						"fontFamily": "Barlow",
                        "wordWrap": true,
                        "transparency":100,
						"color": { "solid": { "color": "#808080"} }, 
                        "visualHeader":[{"show":false}],
                        "bottomBorder": false
				}]}         
        },
        "card":{  
            "*":{        	
                "labels": [{
                    "color": { "solid": { "color": "#A7654A" } },
                    "labelDisplayUnits": 0,
                    "LabelPrecision": 0,
                    "fontSize": 40,
                    "fontFamily": "Barlow"				
                }],			
                "border": [{
                    "show": true,
                    "color": { "solid": { "color": "#F9F4F1"}}, 
                    "radius": 15                
                }],
                "dropShadow":[
                    {
                        "show":true,
                        "color": { "solid": { "color": "#ED5C7B" } }
                    }
                ],
                "background": [{"transparency": 100}],
                "categoryLabels": [{
                    "show" : false
                }],
                
                "title": [{
                    "show": true,
                    "fontColor": { "solid": { "color": "#808080" } },
                        "transparency": 100,
                    "alignment": "center",
                    "fontSize": 15,
                    "fontFamily": "Raleway Thin"
                    }]
            }
        },
        "lineChart":{
            "*":{
                "lineStyles": [{
					"strokeWidth": 3,
					"strokeLineJoin": "bevel",
					"lineStyle": "solid",
					"showMarker": true,
					"markerShape": "circle",
					"markerSize": 6
				}],
                "labels":[
                    {
                        "show":true,
                        "fontSize": 10,
                        "color": { "solid": { "color": "#808080"}}
                    }
                ]
            }
        
        }
    }
}
